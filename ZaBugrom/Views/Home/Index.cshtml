@using ZaBugrom.Managers
@{
    ViewBag.Title = "За бугром.ком";
    Layout = "~/Views/Home/_PostListLayout.cshtml";
}

@{
    foreach (var postData in RepositoryManager.PostRepository.GetList(UserManager.UserId))
